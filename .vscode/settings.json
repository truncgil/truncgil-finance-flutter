{
  "ai-commit.convention": "conventional",
  "ai-commit.message.format": "{type}({scope}): {description}",
  "ai-commit.message.types": [
    "feat",
    "fix",
    "docs",
    "style",
    "refactor",
    "test",
    "chore"
  ],
  "ai-commit.openAI.model": "gpt-3.5-turbo",
  "ai-commit.openAI.temperature": 0.7,
"ai-commit.translation.target": "en",
"java.configuration.updateBuildConfiguration": "automatic"
} 